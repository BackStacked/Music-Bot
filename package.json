{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "A feature-rich Discord music bot built with TypeScript and Discord.js v14",
  "main": "dist/index.js",
  "author": "BackStacked",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BackStacked/Music-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/BackStacked/Music-Bot/issues"
  },
  "homepage": "https://github.com/BackStacked/Music-Bot#readme",
  "keywords": [
    "discord",
    "bot",
    "music",
    "typescript",
    "discord.js",
    "lavalink",
    "audio",
    "streaming"
  ],
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc && tsc-alias",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run copy-assets",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "blue.ts": "^1.3.4-beta",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.3.0",
    "@types/jest": "^29.5.12",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/core": "dist/core",
    "@/utils": "dist/utils",
    "@/commands": "dist/commands",
    "@/events": "dist/events",
    "@/config": "dist/config",
    "@/UI": "dist/UI"
  }
}
